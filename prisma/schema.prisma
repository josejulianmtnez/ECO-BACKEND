// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Users {
  id            Int      @id @default(autoincrement())
  name          String
  email         String   @unique
  role          String
  linked_child  String
  password_hash String
  created_at    DateTime @default(now())
}
